{
  "name": "@devloops/jcron",
  "version": "1.5.0",
  "description": "A high-performance, type-safe job scheduler for Node.js and React Native, inspired by Go's jcron.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "react-native": "dist/index.mjs",
  "browser": "dist/jcron.umd.js",
  "unpkg": "dist/jcron.umd.min.js",
  "jsdelivr": "dist/jcron.umd.min.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./engine": {
      "types": "./dist/engine.d.ts",
      "import": "./dist/engine.mjs",
      "require": "./dist/engine.cjs"
    },
    "./schedule": {
      "types": "./dist/schedule.d.ts",
      "import": "./dist/schedule.mjs",
      "require": "./dist/schedule.cjs"
    },
    "./runner": {
      "types": "./dist/runner.d.ts",
      "import": "./dist/runner.mjs",
      "require": "./dist/runner.cjs"
    },
    "./humanize": {
      "types": "./dist/humanize/index.d.ts",
      "import": "./dist/humanize/index.mjs",
      "require": "./dist/humanize/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:rollup && npm run build:types",
    "build:rollup": "rollup -c rollup.config.js",
    "build:types": "tsc --project tsconfig.build.json --declaration --emitDeclarationOnly --outDir dist",
    "build:tsc": "tsc --project tsconfig.build.json",
    "build:watch": "rollup -c rollup.config.js --watch",
    "clean": "rm -rf dist",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "jest": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "tsc --noEmit --project tsconfig.build.json",
    "prepublishOnly": "npm run build",
    "publish:check": "node scripts/publish.cjs --dry-run",
    "publish:npm": "node scripts/publish.cjs",
    "release": "node scripts/publish.cjs --yes",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "keywords": [
    "cron",
    "scheduler",
    "job",
    "task",
    "typescript",
    "concurrent",
    "react-native",
    "mobile",
    "ios",
    "android"
  ],
  "author": "AI Assistant (Ported from Go)",
  "license": "MIT",
  "dependencies": {
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dayjs": "^1.11.13",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@sinonjs/fake-timers": "^14.0.0",
    "@size-limit/preset-small-lib": "^11.1.6",
    "@types/benchmark": "^2.1.5",
    "@types/bun": "^1.2.18",
    "@types/node": "^24.0.12",
    "@types/sinonjs__fake-timers": "^8.1.5",
    "@types/uuid": "^10.0.0",
    "benchmark": "^2.1.4",
    "bun-types": "^1.2.18",
    "rollup": "^4.28.1",
    "rollup-plugin-dts": "^6.1.1",
    "size-limit": "^11.1.6",
    "ts-node": "10.9.2",
    "tsup": "^8.3.5",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
